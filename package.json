    "@nestjs/throttler": "^2.0.1",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.1",
    "big.js": "^6.2.1",
    "body-parser": "^1.20.2",
    "bull": "^4.12.2",
    "cache-manager": "^5.3.2",
