    "node": ">=18.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts(x)?": [
      "yarn lint --fix"
    ]
  }
